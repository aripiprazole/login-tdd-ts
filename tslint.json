{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {
        "quotes": ["error", "double"]
    },
    "rules": {
        "no-console": false,
        "quotemark": [true, "double"],
        "ter-indent": [true, 4],
        "prettier/prettier": ["error", { "singleQuote": false }]
    },
    "rulesDirectory": []
}
